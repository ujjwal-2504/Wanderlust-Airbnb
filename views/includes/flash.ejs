<% if(success && success.length) { %>
<div
  class="alert alert-success alert-dismissible fade show col-6 offset-3 mt-3"
  role="alert"
>
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(error && error.length) { %>
<div
  class="alert alert-danger alert-dismissible fade show col-6 offset-3 mt-3"
  role="alert"
>
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(warning && warning.length) { %>
<div
  class="alert alert-warning alert-dismissible fade show col-6 offset-3 mt-3"
  role="alert"
>
  <%= warning %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %> <% if(warningWithLink && warningWithLink.length) { %>
<div
  class="alert alert-warning alert-dismissible fade show col-6 offset-3 mt-3"
  role="alert"
>
  <%= warningWithLink[0] %>,
  <a style="text-decoration: none" href="<%= warningWithLink[2] %>"
    ><%= warningWithLink[1] %></a
  >
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
